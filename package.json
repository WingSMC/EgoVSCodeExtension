{
	"name": "ego",
	"displayName": "Ego",
	"description": "Ego language features",
	"repository": {
		"type": "git",
		"url": "https://github.com/WingSMC/EgoVSCodeExtension.git"
	},
	"author": {
		"name": "Gergely Drem√°k",
		"url": "https://github.com/WingSMC"
	},
	"publisher": "WingSMC",
	"license": "MIT",
	"icon": "public/logo.png",
	"version": "2.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "ego",
				"aliases": [
					"Ego",
					"ego"
				],
				"extensions": [
					".ego",
					".lego"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ego",
				"scopeName": "source.ego",
				"path": "./syntaxes/ego.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "ego",
				"path": "./snippets/ego.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package"
	}
}
